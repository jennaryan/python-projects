import time

print("Timer getting ready...... Timer will start in 3 seconds")
print("This is a 10 minute timer, it will tell you when a minute is up until 10 minutes")
time.sleep(3)
print("Timer starting")
time.sleep(60)
print("1 minute has passed")
time.sleep(60)
print("2 minutes have passed")
time.sleep(60)
print("3 minutes have passed")
time.sleep(60)
print("4 minutes have passed")
time.sleep(60)
print("5 minutes have passed")
time.sleep(60)
print("6 minutes have passed")
time.sleep(60)
print("7 minutes have passed")
time.sleep(60)
print("8 minutes have passed")
time.sleep(60)
print("9  minutes have passed")
time.sleep(60)
print("10 minutes have passed")
time.sleep(0.5)
print("BEEP BEEP BEEP TIMER UP!")
           
