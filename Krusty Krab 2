if krusty_krab.startswith(no):
    print("No this is Patrick")
    time.sleep(1)
    krusty_krab2 = input("Is this the Krusty Krab?")
if krusty_krab.startswith(no):
    print("NO this is Patrick!")
    time.sleep(1)
    krusty_krab3 = input("Is this the Krusty Krab?")
